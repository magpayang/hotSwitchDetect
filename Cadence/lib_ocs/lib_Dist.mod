function gaussian_point_from_ber(BitErrorRate): float
-- -----------------------------------------------------------------------------------------
in double     : BitErrorRate       
-------------------------------------------------------------------------------------------
--  This function returns a distribution point on a Gaussian distribution derived for a 
--  specified Bit Error Rate.  The distribution being tested must be Gaussian.
--  The point returned is in terms of sigma or standard deviation.  This function is not valid
--  for processing deterministic distribution points.
-------------------------------------------------------------------------------------------
local float   : p_count     -- 
local lword   : i_count     --
local float   : p_sigma     -- Distribution point for area under Gaussian distribution curve
local double  : Curve_area_d
local float   : Curve_area
local float   : Pass_polarity
local float   : Area_array[9000]
local float   : Area_shift
local float   : Xtrm_array[4]
local integer : marker1
local integer : marker2
local integer : i
body

    Curve_area_d = BitErrorRate    
    if Curve_area_d < 0.5 then
        Pass_polarity  = 1.0
        Curve_area  = float(Curve_area_d)
    else
        Pass_polarity  = -1.0
        Curve_area = float(1.0 - Curve_area_d)
    end_if
    if sigma_table[1] < 499.0e-3 or sigma_table[1] > 0.5 then
        Make_sigma_table
    end_if
    if (BitErrorRate = 0.0) or (BitErrorRate >= 1.0) then 
        p_sigma = -9.0
        println(stdout,"******** INVALID BitErrorRate ",BitErrorRate," **********") 
    else_if (Curve_area < sigma_table[8999]) and (Pass_polarity = 1.0) then 
        p_sigma = -9.0
    else_if (Curve_area < sigma_table[8999])  and (Pass_polarity = -1.0) then 
        p_sigma = 9.0
    else_if Curve_area > sigma_table[2] then
        p_sigma = 0.0
    else
        Area_shift = -Curve_area
        vp_sadd(Area_shift,sigma_table,1,Area_array,1,9000) 
        vp_abs(Area_array,1,Area_array,1,9000) 
        Xtrm_array = xtrm(Area_array)
        marker1 = integer(Xtrm_array[4]) - 1
        marker2 = integer(Xtrm_array[4]) + 1
        p_sigma =   0.002*(Curve_area - sigma_table[marker1] )/(sigma_table[marker1] - sigma_table[marker2]) - float(marker1)/1000.0 
        p_sigma = p_sigma * Pass_polarity       
    end_if
    return(p_sigma)
end_body 



