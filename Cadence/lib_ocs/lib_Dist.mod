function ber_from_gaussian_point(Sigma): float
----------------------
in float    : Sigma
----------------------
----------------------
local
    float   : SigmaPoint
    float   : DeltaSigma
    integer : SigmaInt
    float   : DeltaBer
    float   : Ber
end_local
body
     SigmaPoint = - 1000.0 * Sigma
     SigmaInt   = integer(SigmaPoint)
     DeltaSigma = SigmaPoint - float(SigmaInt)
     Ber        = sigma_table[SigmaInt]
     DeltaBer   = sigma_table[SigmaInt + 1] - Ber
     Ber        = Ber + DeltaSigma * DeltaBer
     return(Ber)
end_body
function rms_from_pk_pk(Pk_Pk_Value,SampleSize): float
-- -----------------------------------------------------------------------------------------
in float      : Pk_Pk_Value    -- Peak to peak spread of sample values
in float      : SampleSize     -- Total number samples in data 
--------------------------------------------------------------------------------------------
--  This function utilizes the spread of sample values and the total number of samples in
--  a distribution of data, where the distribution is assumed to be Gaussian, and then
--  returns the rms value of that distribution.  
--------------------------------------------------------------------------------------------
local float   : p_count     -- 
local lword   : i_count     --
local float   : p_sigma     -- Distribution point for area under Gaussian distribution curve
local double  : Curve_area_d
local float   : Curve_area
local float   : Pass_polarity
local float   : Area_array[9000]
--local float   : Calc_array[9000]
local float   : Area_shift
local float   : Xtrm_array[4]
local integer : marker1
local integer : marker2
local float   : RmsValue
local integer : i
body

    Curve_area_d = 1.0/double(SampleSize)
    Pass_polarity  = -1.0
    Curve_area  = float(Curve_area_d)
    if sigma_table[1] < 499.0e-3 or sigma_table[1] > 0.5 then
        Make_sigma_table
    end_if
     if (Pk_Pk_Value <= 0.0)  then 
        p_sigma = -9.0
        println(stdout,"******** INVALID RMS VALUE ",Pk_Pk_Value," **********") 
    else_if (Curve_area < sigma_table[8999]) then 
        p_sigma = -9.0
    else_if  SampleSize < 3.0 then   
        println(stdout,"******** INVALID SAMPLE SIZE ",SampleSize," **********") 
    else
        Area_shift = -Curve_area
        vp_sadd(Area_shift, sigma_table,1,Area_array,1,9000) 
        vp_abs(Area_array,1,Area_array,1,9000) 
        Xtrm_array = xtrm(Area_array)
        marker1 = integer(Xtrm_array[4]) - 1
        marker2 = integer(Xtrm_array[4]) + 1
        p_sigma =   0.002*(Curve_area - sigma_table[marker1] )/(sigma_table[marker1] - sigma_table[marker2]) - float(marker1)/1000.0 
    end_if
    RmsValue =  Pk_Pk_Value/(2.0 * p_sigma * Pass_polarity)  
    return(RmsValue)
end_body 

function Pk_pk_from_rms ( RmsValue , SampleSize ) : float
-- -----------------------------------------------------------------------------------------
in float            : RmsValue       -- Rms Value of a distribution of data
in float            : SampleSize     -- Total number samples in data 
--------------------------------------------------------------------------------------------
--  This function utilizes the root mean square value of a distribution and the total number of samples in
--  the distribution of data, where the distribution is assumed to be Gaussian, and then
--  returns the P-P value of that distribution.  
--------------------------------------------------------------------------------------------

local float         : p_count     -- 
local lword         : i_count     --
local float         : p_sigma     -- Distribution point for area under Gaussian distribution curve
local double        : curve_area_d
local float         : curve_area
local float         : pass_polarity
local float         : area_array[9000]
local float         : area_shift
local float         : xtrm_array[4]
local integer       : marker1
local integer       : marker2
local float         : p_p_value
local integer       : i

body

    curve_area_d = 1.0 / double ( SampleSize )
    pass_polarity  = -1.0
    curve_area  = float ( curve_area_d )
    if sigma_table [ 1 ] < 499.0e-3 or sigma_table [ 1 ] > 0.5 then
        Make_sigma_table
    end_if
    
    if (RmsValue <= 0.0)  then 
        p_sigma = -9.0
        println( stdout , "******** INVALID RMS VALUE ",RmsValue," **********" ) 
    else_if  SampleSize < 3.0 then   
        println ( stdout , "******** INVALID SAMPLE SIZE ",SampleSize," **********" ) 
    else_if ( curve_area < sigma_table [ 8999 ] ) then 
        p_sigma = -9.0
    else
        area_shift = - curve_area
        vp_sadd ( area_shift , sigma_table , 1 , area_array , 1 , 9000 ) 
        vp_abs ( area_array , 1 , area_array , 1 , 9000 ) 
        xtrm_array = xtrm ( area_array )
        marker1 = integer ( xtrm_array [ 4 ] ) - 1
        marker2 = integer ( xtrm_array [ 4 ] ) + 1
        p_sigma = 0.002 * (curve_area - sigma_table [ marker1 ] ) / ( sigma_table [ marker1 ] - sigma_table [ marker2 ] ) - float ( marker1 ) / 1000.0 
    endif
    p_p_value = 2.0 * p_sigma * pass_polarity * RmsValue 
    return(p_p_value)
    
endbody 
